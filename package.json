{
  "name": "node-module-registry",
  "version": "2.0.4",
  "description": "A Node.JS Module Registry compatible with the NPM and Yarn as well as namespaced packages",
  "scripts": {
    "start": "node registry/server.js",
    "watch": "nodemon registry/server.js | bunyan",
    "test": "npm run test:standard",
    "test:standard": "semistandard registry/*.js registry/**/*.js"
  },
  "author": "Erik Kristensen <erik@erikkristensen.com>",
  "license": "MIT",
  "dependencies": {
    "after": "0.8.2",
    "async": "0.9.2",
    "aws-sdk": "2.133.0",
    "bluebird": "3.5.1",
    "bluebird-co": "2.2.0",
    "bunyan": "1.8.12",
    "github": "13.1.1",
    "github-cache": "2.1.0",
    "github-url-parse": "0.1.0",
    "http-auth-parser": "1.0.0",
    "lodash": "3.10.1",
    "mkdirp": "0.5.1",
    "normalize-package-data": "2.4.0",
    "object-hash": "1.2.0",
    "rc": "1.2.1",
    "request": "2.83.0",
    "restify": "6.0.1",
    "restify-errors": "5.0.0",
    "split": "0.3.3",
    "uuid": "3.1.0",
    "xtend": "4.0.1"
  },
  "devDependencies": {
    "nock": "9.0.22",
    "nodemon": "1.12.1",
    "semistandard": "11.0.0",
    "tape": "4.17.0"
  }
}
